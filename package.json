{
  "name": "vue3-star-ratings",
  "version": "1.0.6",
  "license": "MIT",
  "description": "A simple, customizable vue3 component for star ratings",
  "main": "dist/vue3-star-ratings.ssr.js",
  "browser": "dist/vue3-star-ratings.esm.js",
  "module": "dist/vue3-star-ratings.esm.js",
  "unpkg": "dist/vue3-star-ratings.min.js",
  "types": "vue3-star-ratings.d.ts",
  "files": [
    "dist/*",
    "vue3-star-ratings.d.ts",
    "src/**/*.vue"
  ],
  "keywords": [
    "vue-star",
    "vue3-star",
    "vue-rating",
    "vue 5 star",
    "star rating"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/abiodunolunu/vue3-star-ratings"
  },
  "bugs": {
    "email": "abiodunolunu@gmail.com"
  },
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/compiler-sfc": "^3.0.5",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.2.3",
    "rollup": "^2.36.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^3.8.3",
    "vue": "^3.0.5"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  }
}
